{
	"name": "gamelena/tangerine",
	"description": "Tangerine",
	"version": "1.7.5",
	"type": "library",
	"suggest": {
		"ext-gd": "to use GD library based image processing, you cant had installed ext-imagick ignore this.",
		"ext-imagick": "to use Imagick based image processing, you cant had installed ext-gd ignore this.",
		"ext-xsl": "to generate phpDoc documentation."
	},
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/magento-engcom/zf1-php-7.2-support"
        }
    ],
	"require": {
		"php": ">=5.5.0",
		"ext-libxml": "*",
		"ext-mbstring": "*",
		"ext-dom": "*",
		"ext-mysqli": "*",
		"magento/zendframework1": "dev-master#1.12.16",
		"phpoffice/phpexcel": "1.8.*"
	},
	"require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phploc/phploc": "^2.1.5",
        "pdepend/pdepend": "^2.0.0",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.2",
        "sebastian/phpcpd": "^2.0.0",
        "theseer/phpdox": "^0.9.0",
        "symfony/config": "^2",
        "symfony/finder": "^2",
        "symfony/dependency-injection": "^2",
        "symfony/debug": "^2",
        "symfony/filesystem": "^2"
	},
	"authors": [
		{
			"name": "Rodrigo Riquelme",
			"email": "rodrigo.riquelme.e@gmail.com"
		}
	],
	"bin": [
		"bin/tangerine-create",
		"vendor/magento/zendframework1/bin/zf.sh",
        "vendor/magento/zendframework1/bin/zf.bat"
	],
	"scripts" : {
		"pre-archive-cmd": [
			"bower install"
		]
	},
	"minimum-stability": "stable"
}
