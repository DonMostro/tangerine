{
	"name": "zweicom/admportal",
	"description": "Admportal 1",
	"type": "library",
	"config": {
		"vendor-dir": "/usr/share/php"
	},
	"require": {
		"php": ">=5.4.1"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "zend/zendframework",
				"version": "1.11.9",
				"source": {
					"url": "http://framework.zend.com/svn/framework/standard",
					"type": "svn",
					"reference": "tags/release-1.11.9/"
				},
				"autoload": {
					"psr-0": {
						"Zend": "library",
						"ZendX": "library"
					}
				},
				"config": {
					"bin-dir": "bin"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "PHPOffice/PHPExcel",
				"version": "1.7.8",
				"source": {
					"url": "https://github.com/PHPOffice/PHPExcel.git",
					"type": "git",
					"reference": "PHPExcel_1.7.8"
				},
				"autoload": {
					"psr-0": {
						"PHPExcel": "Classes/"
					}
				}
			}
		}
	],
	"require": {
		"zend/zendframework": "*",
		"PHPOffice/PHPExcel": "*",
		"omissis/zend-db-adapter-mongodb": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"phpunit/phpunit-selenium": ">=1.2",
		"phpunit/dbunit": ">=1.2",
		"phpunit/php-invoker": "1.*",
		"phpunit/phpunit-story": ">=1"
	},
	"authors": [
		{
			"name": "Desarrollo",
			"email": "desarrollo@zweicom.com"
		}
	],
	"minimum-stability": "stable"
}
