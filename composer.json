{
	"name": "gamelena/tangerine",
	"description": "Tangerine",
	"version": "1.6.1",
	"type": "library",
	"require": {
		"php": ">=5.4.1",
		"ext-libxml": "*",
		"ext-mbstring": "*",
		"ext-dom": "*",
		"ext-mysql": "*",
		"zendframework/zendframework1": "~1.12",
		"phpoffice/phpexcel": "1.8.*"
	},
	"suggest": {
		"ext-imagick": "to use Imagick based image processing (or ext-gd).",
		"ext-xsl" : "to generate phpDoc documentation.",
		"ext-xdebug" : "to generate xdebug documentation."
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
	"authors": [
		{
			"name": "Desarrollo",
			"email": "desarrollo@gamelena.com"
		}
	],
	"scripts" : {
		"pre-archive-cmd": [
			"bower install --allow-root",
			"./bower-postinstall.sh"
		]
	},
	"minimum-stability": "stable"
}
