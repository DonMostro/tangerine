{
	"name": "gamelena/tangerine",
	"description": "Tangerine",
	"version": "1.7.0",
	"type": "library",
	"require": {
		"php": ">=5.4.1",
		"ext-libxml": "*",
		"ext-mbstring": "*",
		"ext-dom": "*",
		"ext-mysqli": "*",
		"zendframework/zendframework1": "~1.12",
		"phpoffice/phpexcel": "1.8.*"
	},
	"suggest": {
		"ext-gd": "to use GD library based image processing.",
		"ext-imagick": "to use Imagick based image processing.",
		"ext-xsl": "to generate phpDoc documentation."
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"phploc/phploc": "^2.1.5"
	},
	"authors": [
		{
			"name": "Desarrollo",
			"email": "desarrollo@gamelena.com"
		}
	],
	"scripts" : {
		"pre-archive-cmd": [
			"bower install",
			"./bower-postinstall.sh"
		]
	},
	"minimum-stability": "stable"
}
