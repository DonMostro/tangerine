{
	"name": "zweicom/admportal",
	"description": "Admportal",
	"type": "library",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "zend/zendframework",
				"version": "1.11.9",
				"source": {
					"url": "http://framework.zend.com/svn/framework/standard",
					"type": "svn",
					"reference": "tags/release-1.11.9/"
				},
				"autoload": {
					"psr-0": {
						"Zend": "library",
						"ZendX": "library"
					}
				},
				"config": {
					"bin-dir": "bin"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "PHPOffice/PHPExcel",
				"version": "1.7.8",
				"source": {
					"url": "https://github.com/PHPOffice/PHPExcel.git",
					"type": "git",
					"reference": "PHPExcel_1.7.8"
				},
				"autoload": {
					"psr-0": {
						"PHPExcel": "Classes/"
					}
				}
			}
		}
	],
	"require": {
		"php": ">=5.4.1",
		"ext-libxml": "*",
		"ext-mbstring": "*",
		"ext-dom": "*",
		"ext-gd": "*",
		"zend/zendframework": "*",
		"PHPOffice/PHPExcel": "*"
	},
	"suggest": {
		"ext-gd": "to use GD library based image processing.",
		"ext-imagick": "to use Imagick based image processing."
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"phpunit/phpunit-selenium": ">=1.2",
		"phpunit/dbunit": ">=1.2",
		"phpunit/php-invoker": "1.*",
		"phpunit/phpunit-story": ">=1"
	},
	"authors": [
		{
			"name": "Desarrollo",
			"email": "desarrollo@zweicom.com"
		}
	],
	"scripts" : {
		"pre-archive-cmd": [
			"bower install --allow-root",
			"cp -f -R dojotoolkit/* bower_components/",
			"cp -f -R bower_components/dojo-calendar/* bower_components/dojox/calendar/"
		 ]
	}
}
