<?xml version="1.0" encoding="utf-8" ?>
<phpdox xmlns="http://xml.phpdox.net/config">
 <project name="tangerine" source="${basedir}" workdir="${basedir}/tests/phpdox/xml">
  <collector publiconly="false">
   <include mask="*.php" />
  </collector>

  <generator output="${basedir}/tests/phpdox">
   <build engine="html" enabled="true" output="${basedir}/tests/api">
    <file extension="html" />
   </build>
  </generator>
 </project>
</phpdox>